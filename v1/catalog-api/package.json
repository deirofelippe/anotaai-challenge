{
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "mongodb": "^6.5.0",
    "prom-client": "^15.1.3",
    "rabbitmq-client": "^4.5.4",
    "redis": "^4.6.12",
    "response-time": "^2.3.3",
    "yup": "^1.4.0"
  },
  "engines": {
    "node": ">= 20.15.0"
  },
  "type": "commonjs",
  "scripts": {
    "dev": "NODE_ENV=dev LOG_LEVEL=debug npx nodemon --config ./nodemon.json --exec 'node -r @swc-node/register src/server.ts'",
    "prestart": "npm run build",
    "start": "NODE_ENV=production node build/src/server.js",
    "test": "NODE_ENV=test DISABLE_LOGS=true npx jest --runInBand -c ./jest.config.swc.js",
    "test:cov": "npm run test -- --coverage",
    "test:ci": "CI=true DISABLE_LOGS=true npm run test -- --coverage",
    "test:debug:swc": "NODE_ENV=test node --inspect-brk ./node_modules/.bin/jest -c ./jest.config.swc.js --runInBand __tests__",
    "test:debug": "NODE_ENV=test NODE_OPTIONS='--inspect' npx jest -c ./jest.config.tsjest.js --runInBand --coverage false __tests__",
    "build": "npx swc ./src -d ./build --config-file ./.swcrc",
    "prelint": "sed -i 's/\"type\": \"commonjs\"/\"type\": \"module\"/g' ./package.json",
    "lint": "npx eslint ./",
    "postlint": "sed -i 's/\"type\": \"module\"/\"type\": \"commonjs\"/g' ./package.json"
  },
  "devDependencies": {
    "@eslint/js": "^9.3.0",
    "@faker-js/faker": "^8.4.1",
    "@swc-node/register": "^1.8.0",
    "@swc/cli": "^0.3.6",
    "@swc/core": "^1.4.6",
    "@swc/jest": "^0.2.36",
    "@types/eslint-plugin-security": "^3.0.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.16",
    "@types/response-time": "^2.3.8",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint-plugin-jest": "^28.5.0",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-security": "^3.0.0",
    "eslint-plugin-unicorn": "^53.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.1",
    "nodemon": "^3.1.0",
    "prettier": "3.2.4",
    "sonarqube-scanner": "^4.2.3",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.9.0"
  }
}
